/* 自适应布局方案 */
.custom-nav {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1000;
  background-color: #faf9f5;
}

.nav-content {
  display: flex;
  align-items: center;
  justify-content: center;
  background: #faf9f5;
}

.nav-left,
.nav-right {
  position: absolute;
  flex-shrink: 0;
  display: flex;
  align-items: center;
  /* box-shadow: 0 0 0 2rpx red; */
  justify-content: center;
  padding: 0 24rpx;
}

.nav-title {
  flex: 1;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: 500;
  font-size: 32rpx;
}
